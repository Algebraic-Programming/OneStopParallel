@PACKAGE_INIT@

# Add the directory containing this file to the CMAKE_MODULE_PATH
# so that find_dependency() can find our custom FindCOPT.cmake module.
list(APPEND CMAKE_MODULE_PATH "${CMAKE_CURRENT_LIST_DIR}")

include(CMakeFindDependencyMacro)

if(@OSP_DEPENDS_ON_OPENMP@)
    find_dependency(OpenMP REQUIRED)
endif()
if(@OSP_DEPENDS_ON_BOOST_GRAPH@)
    find_dependency(Boost 1.71.0 REQUIRED COMPONENTS graph)
endif()
if(@COPT_FOUND@)
    find_dependency(COPT)
endif()
if(@Eigen3_FOUND@)
    find_dependency(Eigen3 3.4)
endif()

include("${CMAKE_CURRENT_LIST_DIR}/OneStopParallelTargets.cmake")